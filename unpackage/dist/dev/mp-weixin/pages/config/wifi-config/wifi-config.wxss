
.img-a {
		position: absolute;
		width: 100%;
		/* top: -280rpx; */
		right: -100rpx;
}
.img-b {
		position: absolute;
		width: 50%;
		bottom: 0;
		left: -50rpx;
		margin-bottom: -200rpx;
}
.t-login {
		width: 600rpx;
		margin: 0 auto;
		font-size: 28rpx;
		color: #000;
}
.t-login button {
		font-size: 28rpx;
		background: #2796f2;
		color: #fff;
		height: 90rpx;
		line-height: 90rpx;
		border-radius: 50rpx;
		font-weight: bold;
}
.t-b {
		text-align: left;
		font-size: 46rpx;
		color: #000;
		padding: 120rpx 0 120rpx 0;
		font-weight: bold;
}
.t-login input {
		padding: 0 20rpx 0 120rpx;
		height: 80rpx;
		line-height: 90rpx;
		margin-bottom: 50rpx;
		background: #f8f7fc;
		border: 1px solid #e9e9e9;
		font-size: 28rpx;
		border-radius: 50rpx;
}
.t-login .t-a {
		position: relative;
}
.t-login .t-a ._span {
		position: absolute;
		top: 50%;
		-webkit-transform: translateY(-50%);
		        transform: translateY(-50%);
		left: 10rpx;
		font-size: 12px;
		width: 75px;
		display: inline-block;
		text-align: center;
}
 .uni-input-wrapper {
		left: 20px;
}

